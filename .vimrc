syntax on
set number
filetype plugin indent on
set autoread
set ruler
set hid
set ignorecase
set smartcase
set lazyredraw 
set encoding=utf8
set nobackup
set nowb
set noswapfile
set textwidth=80
set colorcolumn=+1
set number relativenumber
set nu rnu
set ai 
set si
set wrap
set hlsearch 
set ignorecase
set incsearch
set spell spelllang=en_us
noremap <Up> <Nop>
noremap <Down> <Nop>
noremap <Left> <Nop>
noremap <Right> <Nop>

set rtp+=/home/linuxbrew/.linuxbrew/opt/fzf
